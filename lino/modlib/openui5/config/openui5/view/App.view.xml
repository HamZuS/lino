<mvc:View
        controllerName="sap.ui.demo.wt.controller.App"
        xmlns="sap.m"
        xmlns:tnt="sap.tnt"
        xmlns:u="sap.ui.unified"
       	xmlns:core="sap.ui.core"
        xmlns:mvc="sap.ui.core.mvc"
        xmlns:customData="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
        xmlns:html="http://www.w3.org/1999/xhtml"
        displayBlock="true" height="100%">
    <!--<App class="sapUiDemoWT">-->

        <!--<tnt:ToolPage id="toolPage">-->

        <!--<tnt:header>-->

        <Page title="helloWorld" showHeader="false" enableScrolling="false" id="mainPage">
                <tnt:ToolHeader id="mainmenu">
                        <!--<ToolbarSpacer width="20px" />-->
                        {% for item in menu.items %}
                        <Button id="menu.{{ item.name }}" text="{{ item.label }}" type="Transparent"
                                press="handlePressOpenMenu"
                                customData:menu="{{item.name}}"
                        >
                            <!--<layoutData>-->
                            <!--<OverflowToolbarLayoutData priority="Low" />-->
                            <!--</layoutData>-->
                        </Button>
                        {% endfor %}
                        <tnt:ToolHeaderUtilitySeparator />
                        <ToolbarSpacer>
                            <layoutData>
                                <OverflowToolbarLayoutData priority="NeverOverflow" minWidth="20px" />
                            </layoutData>
                        </ToolbarSpacer>
                        {% if ar.user.is_authenticated() %}
                        <Button text="{{ar.user.name}}" type="Transparent"
                                press="handlePressOpenMenu"
                                customData:menu="user.user">

                            <layoutData>
                                <OverflowToolbarLayoutData priority="NeverOverflow" />
                            </layoutData>
                        </Button>
                        {% else %}
                        <Button text="Sign in" type="Transparent" press="onSignInButtonPress">
                            <layoutData>
                                <OverflowToolbarLayoutData priority="NeverOverflow" />
                            </layoutData>
                        </Button>

                        {% endif %}
                    </tnt:ToolHeader>

                <headerContent>
                    <Button
                            icon="sap-icon://hello-world"
                            press="onOpenDialog"/>
                </headerContent>
                <!--<u:Header enabledPrevious="true">-->

                <!--</u:Header>-->
                <content>
                    <NavContainer height="100%" id="viewport" >
                        <pages>
                            <Page showHeader="true" >
                                <Panel ><core:HTML id="dashboard"
                                        content=""
                                />

                                </Panel>
                                <!--<mvc:XMLView viewName="sap.ui.demo.wt.view.HelloPanel"/>-->
                                <!--<mvc:XMLView viewName="sap.ui.demo.wt.view.SideNav"/>-->
                                <!--<mvc:XMLView viewName="sap.ui.demo.wt.view.HelloPanel"/>-->
                                <!--<mvc:XMLView viewName="sap.ui.demo.wt.view.AllTickets"/>-->
                            </Page>
                        </pages>
                    </NavContainer>
                </content>

            </Page>

        <!--</tnt:mainContents>-->


        <!--</tnt:ToolPage>-->


    <!--</App>-->
</mvc:View>