<mvc:View
		controllerName="lino.controller.detail"
		xmlns="sap.m"
		xmlns:m="sap.m"
		xmlns:mvc="sap.ui.core.mvc"
		xmlns:core="sap.ui.core"
		xmlns:u="sap.ui.unified"
		xmlns:customData="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
		xmlns:html="http://www.w3.org/1999/xhtml"
		xmlns:ui="sap.ui"
		xmlns:editor="openui5"
		height="100%"
		class="sapUiSizeCompact">
	<Page
		showHeader="true"
		showNavButton="true"
		navButtonPress="onNavButtonPress"
		showFooter="true"
		enableScrolling="false"
		title="{record>/title}"
		id="MAIN_PAGE"
		customData:PK="{{actor.model._meta.pk.name}}"
		customData:actor_id="{{actor.actor_id}}"
		customData:url="/restful/{{actor.app_label}}/{{actor.__name__}}/"
	>
		<content>
							{% include main.oui5_field_template %}
		</content>
	</Page>
</mvc:View>